{
	"name": "codetss",
	"displayName": "Code TSS",
	"description": "",
	"version": "0.0.6",
	"publisher": "tss",
	"engines": {
		"vscode": "^1.52.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./src/extension.js",

	"contributes": {

		"configuration": {
			"title": "tss",
			"properties": {
			  "tss.host": {
				"type": "string",
				"default": "tss.co.ua",
				"description": "Tss host."
			  },
			  "tss.port": {
				"type": "number",
				"default": 5556,
				"description": "Tss port."
			  }
			}
		},

		"commands": [
			{
				"command": "codetss.pinCommand",
				"title": "  PIN"
			},
			{
				"command": "codetss.checkCommand",
				"title": " CHECK"
			},
			{
				"command": "codetss.helpCommand",
				"title": "HELP"
			}

		],
		
		"menus": {
			"editor/title": [
				{
				  "command": "codetss.pinCommand",
				  "group": "navigation"
				},
				{
					"command": "codetss.checkCommand",
					"group": "navigation"
				},
				{
					"command": "codetss.helpCommand",
					"group": "navigation"
				}
 
			]
		}
	},

	"languages": [
		{
		  "id": "python",
		  "extensions": [".py"],
		  "aliases": ["Python", "py"],
		  "filenames": [],
		  "firstLine": "^#!/.*\\bpython[0-9.-]*\\b",
		  "configuration": "./language-configuration.json"
		}
	  ],
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
